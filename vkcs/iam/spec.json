{
	"datasources": [
		{
			"name": "service_user",
			"schema": {
				"description": "Use this data source to get information about an IAM service user.",
				"attributes": [
					{
						"name": "id",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "ID of the service user."
						}
					},
					{
						"name": "name",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "Name of the service user."
						}
					},
					{
                        "name": "region",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "description": "The region in which to obtain the IAM Service Users client. If omitted, the `region` argument of the provider is used."
                        }
                    },
					{
						"name": "created_at",
						"string": {
							"computed_optional_required": "computed",
							"description": "Service user creation timestamp."
						}
					},
					{
						"name": "creator_name",
						"string": {
							"computed_optional_required": "computed",
							"description": "Name of the user who created the service user."
						}
					},
					{
						"name": "description",
						"string": {
							"computed_optional_required": "computed",
							"description": "Service user description."
						}
					},
					{
						"name": "role_names",
						"list": {
							"computed_optional_required": "computed",
							"element_type": {
								"string": {}
							},
							"description": "Names of roles assigned to the service user."
						}
					}
				]
			}
		}
	],
	"provider": {
		"name": "vkcs"
	},
	"resources": [
		{
			"name": "service_user",
			"schema": {
				"description": "Manages an IAM service user within VKCS.",
				"attributes": [
					{
						"name": "description",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "Description of the service user. The maximum length is 256 characters. Changing this creates a new resource.",
							"plan_modifiers": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
											}
										],
										"schema_definition": "stringplanmodifier.RequiresReplaceIfConfigured()"
									}
								}
							],
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.LengthAtMost(256)"
									}
								}
							]
						}
					},
					{
						"name": "name",
						"string": {
							"computed_optional_required": "required",
							"description": "Name of the service user. The name must be unique. The length must be between 3 and 32 characters. Changing this creates a new resource.",
							"plan_modifiers": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
											}
										],
										"schema_definition": "stringplanmodifier.RequiresReplace()"
									}
								}
							],
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.LengthBetween(3, 32)"
									}
								},
								{
									"custom": {
										"imports": [
											{
												"path": "regexp"
											},
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.RegexMatches(regexp.MustCompile(\"^[0-9a-zA-Z_-]+$\"), \"\")"
									}
								}
							]
						}
					},
					{
                        "name": "region",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "description": "The region in which to obtain the IAM Service Users client. If omitted, the `region` argument of the provider is used. Changing this creates a new resource.",
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "stringplanmodifier.RequiresReplaceIfConfigured()"
                                    }
                                }
                            ]
                        }
                    },
					{
						"name": "role_names",
						"list": {
							"computed_optional_required": "required",
							"description": "Names of roles assigned to the service user. Changing this creates a new resource.",
							"element_type": {
								"string": {}
							},
							"plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/listplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "listplanmodifier.RequiresReplaceIfConfigured()"
                                    }
                                }
                            ],
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/listvalidator"
											}
										],
										"schema_definition": "listvalidator.SizeAtLeast(1)"
									}
								}
							]
						}
					},
					{
						"name": "created_at",
						"string": {
							"computed_optional_required": "computed",
							"description": "Service user creation timestamp."
						}
					},
					{
						"name": "creator_name",
						"string": {
							"computed_optional_required": "computed",
							"description": "Name of the user who created the service user."
						}
					},
					{
						"name": "id",
						"string": {
							"computed_optional_required": "computed",
							"description": "ID of the service user."
						}
					},
					{
						"name": "login",
						"string": {
							"computed_optional_required": "computed",
							"description": "Login name of the service user."
						}
					},
					{
						"name": "password",
						"string": {
							"computed_optional_required": "computed",
							"description": "Password of the service user. _note_ This is a sensitive attribute.",
							"sensitive": true
						}
					}
				]
			}
		}
	],
	"version": "0.1"
}
